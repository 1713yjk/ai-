{
  "version": 2,
  "name": "az-rings-ai-proxy",
  "functions": {
    "api/bailian.js": {
      "maxDuration": 30
    },
    "api/chat.js": {
      "maxDuration": 60
    },
    "api/upload.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
